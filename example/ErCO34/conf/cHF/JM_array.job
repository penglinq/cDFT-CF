#!/bin/bash
#SBATCH --qos=shared 
#SBATCH --nodes=1
#SBATCH --ntasks=32
#SBATCH --cpus-per-task=2
#SBATCH --time=10:00:00
#SBATCH --constraint=cpu
#SBATCH --output=run_%a/slurm_191019_J7.5.out
#SBATCH --mail-type=END,FAIL
#SBATCH --array=0-99

module list
pwd
date
srun hostname
cd "run_"$SLURM_ARRAY_TASK_ID
srun -n 32 python -u ./get_JM.py
cd ../
date


